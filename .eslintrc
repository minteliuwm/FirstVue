{
  "parserOptions": {
    "ecmaVersion": 6
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "commonjs": true,
    "jquery": true
  },
  "globals": {
    "ga": true,
    "define": false,
    "require": false,
    "Promise": false,
    "requireAsync": false,
    "m": false,
    "router": false
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [0],
    "no-extra-boolean-cast": 1,
    "no-console": 1,
    "eqeqeq": 1,
    "comma-dangle": 1,
    "no-unused-vars": 1,
    "no-empty": 1,
    "no-extra-semi": 1,
    "no-irregular-whitespace": 1,
    "quotes": [0]
  }
}
